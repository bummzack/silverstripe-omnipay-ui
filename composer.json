{
	"name": "bummzack/silverstripe-omnipay-ui",
	"description": "SilverStripe Omnipay UI Module",
	"type": "silverstripe-vendormodule",
	"license": "BSD-3-Clause",
	"keywords": ["silverstripe","payment","omnipay"],
	"require": {
		"silverstripe/silverstripe-omnipay": "^3.0 | dev-main#8f277fa5ef513ded38e55ed3c56778feada26174",
        "silverstripe/admin": "^1.0 | ^2.0"
	},
    "require-dev": {
        "phpunit/phpunit": "^5.7 | ^9"
    },
    "extra" : {
		"installer-name": "omnipay-ui",
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "expose": [
            "client/dist"
        ]
    },
    "scripts": {
        "lint": "phpcs -s src/ tests/"
    },
    "autoload": {
        "psr-4": {
            "Bummzack\\SsOmnipayUI\\": "src/",
            "Bummzack\\SsOmnipayUI\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
